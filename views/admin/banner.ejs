
<html lang="en">
    <%- include('../../partials/admin/head.ejs')%>
<body>
 
   <%- include('../../partials/admin/header.ejs')%>
    <section id="main">
        <div class="container">
            <div class="row">
                    <%- include('../../partials/admin/left.ejs')%>
                <div class="col-md-9">
                    <h2>Banner Management</h2>
                    <%if(msg!='mssg'){%>

                        <p><%=msg%></p>
                        <%}%>
                    
                    <table class="table table-hover m-2">
                        
                        <tr>
                            <th>banner Title</th>
                            <th>banner Description</th>
                            <th>banner Img</th>
                            <th>banner More Details</th>
                            <th>banner Update</th>
                        </tr>
                        <tr>
                            <td><%=data.title%></td>
                            <td style="text-align: justify;"><%=data.desc%></td>
                            <td><img src="/upload/<%=data.img%>" alt="" style="width: 70px;"></td>
                            <td style="text-align: justify;"><%=data.mdetails%></td>
                            <td><a href="/admin/bannerform/<%=data.id%>"><button class="btn btn-primary ">Update</button></a></td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </section>
</body>
</html>